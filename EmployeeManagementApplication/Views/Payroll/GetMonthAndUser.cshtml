@model EmployeeManagementApplication.Models.GetDate

@{
    ViewData["Title"] = "GetMonthAndUser";
}

<h1 style="padding-top:5%">Get Payroll By Year</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GetMonthAndUser">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Date" class="control-label">Select a Year</label>
                <input asp-for="Date" class="form-control" required/>
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
                <a class="btn btn-primary me-2" asp-area="" asp-action="Index">Back to Index</a>

            </div>
        </form>
    </div>
</div>


